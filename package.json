{
  "name": "resume-assistant",
  "version": "1.0.0",
  "description": "Weex Resume Assistant, powered by Vue.js.",
  "main": "index.js",
  "keywords": [
    "weex",
    "vue",
    "resume",
    "javascript",
    "android",
    "ios"
  ],
  "scripts": {
    "build": "webpack",
    "dev": "webpack --watch",
    "copy:android": "cp dist/index.weex.js android/app/src/main/assets/index.js",
    "copy:ios": "cp dist/index.weex.js ios/assets/index.js",
    "copy": "npm run copy:android && npm run copy:ios",
    "serve": "serve -p 1337",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=4.0"
  },
  "author": "Myth",
  "license": "Apache-2.0",
  "dependencies": {
    "fs-extra": "^4.0.3",
    "vue": "^2.5.13",
    "weex-html5": "^0.4.1",
    "weex-vue-render": "^1.0.13"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-component": "^1.1.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "postcss-plugin-px2rem": "^0.7.0",
    "postcss-plugin-weex": "^0.1.6",
    "serve": "^1.4.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^12.2.2",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1",
    "weex-loader": "^0.6.9",
    "weex-vue-precompiler": "^0.1.17"
  }
}
